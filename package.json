{
  "name": "dart-node-sdk",
  "version": "0.0.1",
  "description": "dart api implementation in node",
  "main": "dist/public-api.js",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "prebuild": "npm run clean",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "test": "jest",
    "clean": "./node_modules/.bin/rimraf dist",
    "prepare": "husky install",
    "commit": "./node_modules/.bin/cz",
    "lint": "eslint . --ext .js,.ts",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "gen": "babel-node -x .ts -- ./tools/scripts/generate.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jayjnu/dart-node-sdk.git"
  },
  "keywords": [
    "dart",
    "재무제표",
    "sdk",
    "api"
  ],
  "author": "Jinwoo Jung <doublejnu@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jayjnu/dart-node-sdk/issues"
  },
  "homepage": "https://github.com/jayjnu/dart-node-sdk#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.14.3",
    "@babel/node": "^7.14.2",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-typescript": "^7.13.0",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/ejs": "^3.0.6",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "colors": "^1.4.0",
    "cz-conventional-changelog": "^3.3.0",
    "ejs": "^3.1.6",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^6.0.0",
    "jest": "^27.0.3",
    "lint-staged": "^11.0.0",
    "msw": "^0.29.0",
    "prettier": "2.3.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.2",
    "yargs": "^16.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{ts,js}": "eslint --cache --fix",
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "got": "^11.8.2"
  }
}
